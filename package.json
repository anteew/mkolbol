{
  "name": "mkolbol",
  "version": "0.1.0",
  "description": "Microkernel for AI agent systems built on Model Context Protocol (MCP)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./kernel": {
      "import": "./dist/kernel/index.js",
      "types": "./dist/kernel/index.d.ts"
    },
    "./transports": {
      "import": "./dist/transports/index.js",
      "types": "./dist/transports/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./control": {
      "import": "./dist/control/index.js",
      "types": "./dist/control/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run --reporter=default",
    "test:watch": "vitest",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "microkernel",
    "mcp",
    "model-context-protocol",
    "ai-agents",
    "json-rpc"
  ],
  "author": "Dan Mann",
  "license": "MIT",
  "dependencies": {
    "itty-router": "^4.0.27"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "tslib": "^2.6.2",
    "typescript": "^5.6.2",
    "vitest": "^1.6.0"
  }
}
