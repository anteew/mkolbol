+        usage: 'mk dev <config> [--verbose]',
+        handler: async (args) => {
+            if (args.length === 0) {
+                console.error('Error: Missing topology config file');
+                console.error('Usage: mk dev <config> [--verbose]');
+                return EXIT_USAGE;
+            }
