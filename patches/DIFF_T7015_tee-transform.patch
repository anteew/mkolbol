+        return this.tokens;
+    }
+    getPendingCount() {
+        return this.pendingMessages.length;
+    }
+    stop() {
+        if (this.refillTimer) {
+            clearInterval(this.refillTimer);
+            this.refillTimer = undefined;
+        }
+    }
