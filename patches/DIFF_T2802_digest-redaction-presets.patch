+export interface DigestEvent {
+  ts: number;
+  lvl: string;
+  case: string;
+  phase?: string;
+  evt: string;
+  id?: string;
+  corr?: string;
+  path?: string;
+  payload?: unknown;
+}
+
+export interface SuspectEvent extends DigestEvent {
+  score: number;
+  reasons: string[];
+}
+
+export interface DigestOutput {
+  case: string;
+  status: 'fail';
+  duration: number;
+  location: string;
+  error?: string;
+  summary: {
+    totalEvents: number;
+    includedEvents: number;
+    redactedFields: number;
+    budgetUsed: number;
+    budgetLimit: number;
+  };
+  suspects?: SuspectEvent[];
+  codeframes?: CodeFrame[];
+  events: DigestEvent[];
