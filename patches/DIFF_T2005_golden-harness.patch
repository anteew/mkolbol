+        const onData = (m) => {
+            if (this.eventLogger) {
+                const payloadSize = JSON.stringify(m).length;
+                this.eventLogger({
+                    ts: Date.now(),
+                    lvl: 'debug',
+                    case: 'control-bus',
+                    evt: 'subscribe',
+                    payload: { topic, payloadSize },
+                });
+            }
+            handler(m);
+        };
         t.on('data', onData);
         return () => t.off('data', onData);
     }
